{
	"name": "resume",
	"version": "0.0.1",
	"scripts": {
		"dev": "concurrently --kill-others \"npm run dev:svelte\" \"npm run dev:css\"",
		"dev:svelte": "svelte-kit dev",
		"dev:css": "sass --watch style/main.scss:static/css/main.css",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"build": "npm run build:css && npm run build:svelte",
		"build:css": "sass style/main.scss:static/css/main.css --style compressed",
		"build:svelte": "rm -rf build && svelte-kit build && touch .svelte-kit/static/build/.nojekyll",
		"deploy": "npm run build && npx gh-pages -d .svelte-kit/static/build -t true"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-free": "^5.15.4",
		"@sveltejs/adapter-static": "^1.0.0-next.14",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^4.19.0",
		"@typescript-eslint/parser": "^4.19.0",
		"concurrently": "^6.2.0",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"gh-pages": "^3.2.3",
		"normalize.css": "^8.0.1",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "^2.2.0",
		"sass": "^1.37.5",
		"svelte": "^3.34.0",
		"svelte-check": "^2.0.0",
		"svelte-preprocess": "^4.0.0",
		"tslib": "^2.0.0",
		"typescript": "^4.0.0"
	},
	"type": "module"
}
